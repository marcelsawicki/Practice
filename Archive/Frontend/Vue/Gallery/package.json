{
  "name": "my-project",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run build:parts && npm run process:html",
    "build:parts": "npm run compile",
    "clean": "rimraf dist && rimraf coverage",
    "compile": "webpack --config config/webpack.config.prod.js",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others \"webpack-dev-server --hot --inline\"",
    "lint": "tslint app/**/*.ts",
    "process:html": "htmlprocessor app/index.html -o dist/index.html",
    "serve": "http-server dist/ -o"
  },
  "dependencies": {
    "@angular/core": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@types/jquery.contextmenu": "^1.7.31",
    "axios": "~0.15.3",
    "inversify": "^4.1.1",
    "jquery-ts": "^2.1.1",
    "vue": "~2.2.4",
    "vue-class-component": "~5.0.0",
    "vue-context-menu": "^2.0.2",
    "vue-property-decorator": "^4.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.45",
    "@types/mocha": "~2.2.40",
    "@types/node": "~7.0.8",
    "@types/sinon": "~1.16.36",
    "awesome-typescript-loader": "~3.1.2",
    "chai": "~3.5.0",
    "clean-css-cli": "~4.0.9",
    "compression-webpack-plugin": "~0.3.2",
    "concurrently": "~3.4.0",
    "copy-webpack-plugin": "~4.0.1",
    "cross-env": "~3.2.4",
    "es6-promise": "~4.1.0",
    "file-loader": "~0.10.1",
    "globby": "~6.1.0",
    "htmlprocessor": "~0.2.4",
    "http-server": "~0.9.0",
    "istanbul-instrumenter-loader": "~2.0.0",
    "json-loader": "~0.5.4",
    "lodash.merge": "~4.6.0",
    "minimist": "~1.2.0",
    "mocha": "~3.2.0",
    "ncp": "~2.0.0",
    "node-sass": "~4.5.0",
    "opn-cli": "~3.1.0",
    "phantomjs-prebuilt": "~2.1.14",
    "raw-loader": "~0.5.1",
    "remap-istanbul": "~0.9.1",
    "rimraf": "~2.6.1",
    "sinon": "~2.1.0",
    "tslint": "~4.5.1",
    "tslint-loader": "~3.4.3",
